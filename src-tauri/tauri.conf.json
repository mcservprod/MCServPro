{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "mcservpro",
  "version": "0.0.1",
  "identifier": "com.mcservpro.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "mcservpro ( ALPHA v0.0.1 )",
        "width": 512,
        "height": 850,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.jpg",
      "icons/icon.ico"
    ]
  }
}
